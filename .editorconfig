root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = false
max_line_length = 120
tab_width = 4

[*.proto]
indent_size = 2
tab_width = 2

[*.yml]
indent_size = 2

[{*.go, *.go2}]
indent_style = tab

[{*.har, *.jsb2, *.jsb3, .babelrc, .eslintrc, .stylelintrc, bowerrc, jest.config}]
indent_size = 2

[{*.pb, *.textproto}]
indent_size = 2
tab_width = 2

[Makefile]
indent_style = tab
